{
    "name": "10bis Form Filler",
    "version": "1.1",
    "manifest_version": 2,
    "description": "10bis Form Filler",
    "background": {
        "scripts": ["content.js"],
        "persistent": true
    },
    "permissions": [
        "declarativeContent",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["http://www.10bis.co.il/*"],
            "js": ["injected.js"]
        }
    ],
    "page_action": {
        "default_icon": "icon.png"
    }
}
