{
  "name": "10bis form filler",
  "version": "1.0",
  "manifest_version": 2,
  "description": "10bis form filler",
  "background": {
    "scripts": ["buttonhook.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
          "http://*/*",
	      "https://*/*"
      ],
      "all_frames" : true
    }
  ],
  "web_accessible_resources" : [
      "injected.js"
  ],
  "permissions": [
     "http://locahost:2907/*"
  ]
}
